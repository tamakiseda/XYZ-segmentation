{
    "classification": {
        "RandomForestClassifier": {
            "n_estimators": 100,
            "criterion": {
                "options": [
                    "gini",
                    "entropy",
                    "log_loss"
                ],
                "default": "gini"
            },
            "max_depth": 12,
            "min_samples_split": 2
        }
    },
    "cluster": {
        "KMeans": {
            "n_clusters": 3,
            "init": {
                "options": [
                    "k-means++",
                    "random"
                ],
                "default": "k-means++"
            },
            "max_iter": 300,
            "random_state": 42
        }
    },
    "regression": {
        "Lasso": {
            "alpha": 1.05,
            "random_state": null
        },
        "RandomForestRegressor": {
            "n_estimators": 100,
            "criterion": {
                "options": [
                    "squared_error",
                    "absolute_error",
                    "friedman_mse",
                    "poisson"
                ],
                "default": "squared_error"
            },
            "max_depth": null,
            "min_samples_split": 2,
            "min_samples_leaf": 1
        },
        "DecisionTreeRegressor": {
            "criterion": {
                "options": [
                    "squared_error",
                    "friedman_mse",
                    "absolute_error",
                    "poisson"
                ],
                "default": "squared_error"
            },
            "splitter": {
                "options": [
                    "best",
                    "random"
                ],
                "default": "best"
            },
            "max_depth": null,
            "min_samples_split": 2
        }
    }
}
